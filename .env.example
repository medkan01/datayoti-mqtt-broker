# Configuration Base de données PostgreSQL/TimescaleDB
PG_USER=postgres
PG_PASSWORD=your_postgres_password
PG_DATABASE=datayoti_db

# Configuration utilisateurs de base de données
MQTT_INGESTOR_PASSWORD=your_mqtt_ingestor_password
GRAFANA_READER_PASSWORD=your_grafana_reader_password

# Configuration MQTT (optionnel si le broker ne nécessite pas d'auth)
MQTT_USER=datayoti_monitor
MQTT_PASSWORD=your_mqtt_password

# Configuration Grafana
GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=your_grafana_admin_password

# Configuration du collecteur de données (pour développement local)
MQTT_SERVER=192.168.0.x
MQTT_COLLECTOR_USER=datayoti_monitor
MQTT_COLLECTOR_PASSWORD=your_collector_password